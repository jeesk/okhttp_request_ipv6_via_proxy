<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config>
        <trust-anchors>
            <certificates src="system" />
            <!--            通过配置该设置可以忽略用户的证书-->
            <certificates src="user" />
        </trust-anchors>
    </base-config>
</network-security-config>